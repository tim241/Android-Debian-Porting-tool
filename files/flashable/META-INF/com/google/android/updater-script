ui_print("------------------------------------------");
ui_print("-----------Debian-Porting-tool------------");
ui_print("---------------Installer------------------");
ui_print("------------------------------------------");
show_progress(0.500000, 0);
set_progress(0.2);
ui_print("------------------------------------------");
ui_print("----------------Extracting----------------");
ui_print("------------------------------------------");
package_extract_dir("tools", "/tmp");
package_extract_dir("sdcard", "/tmp/linux_chroot/");
ui_print("------------------------------------------");
ui_print("-----------Setting-Permissions------------");
ui_print("------------------------------------------");
set_perm(0, 0, 0777, "/tmp/extract.sh");
set_perm(0, 0, 0777, "/tmp/mount.sh");
set_progress(0.5);
ui_print("------------------------------------------");
ui_print("--------------Mounting-sdcard-------------");
ui_print("------------------------------------------");
run_program("/tmp/mount.sh");
mount("ext4","ext4","/dev/block/mmcblk1p2","/tmp/linux_chroot");
set_progress(0.6);
set_progress(0.7);
ui_print("------------------------------------------");
ui_print("-------------Extracting-Chroot------------");
ui_print("------------------------------------------");
run_program("/tmp/extract.sh");
set_progress(0.8);
ui_print("------------------------------------------");
ui_print("----------------Generated-----------------");
ui_print("--------------------By--------------------");
ui_print("-------Android-Debian-Porting-toolÂ©-------");
ui_print("------------------------------------------");
set_progress(0.9);
set_progress(1.0);
